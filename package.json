{
  "name": "collegechalo-website",
  "version": "1.0.0",
  "description": "College Chalo - College discovery and admissions platform",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest --passWithNoTests",
    "seed-mongo": "ts-node --transpile-only scripts/seed-mongo.ts",
    "import-localstorage": "ts-node --transpile-only scripts/import-localstorage.ts",
    "create-indexes": "ts-node --transpile-only scripts/create-indexes.ts",
    "start:prod": "next start -p 3000",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop ecosystem.config.js",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\" || true",
    "type-check": "tsc --noEmit"
    ,
    "test:e2e": "playwright test --project=chromium"
  },
  "dependencies": {
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "next": "16.1.6",
    "mongodb": "^5.8.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "typescript": "^5.0.0"
  },
  "devDependencies": {
    "ts-node": "^10.9.1",
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/jsonwebtoken": "^9.0.2",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.3.0",
    "eslint": "^9.0.0",
    "eslint-config-next": "16.1.6",
    "pm2": "^5.2.0"
  ,
    "jest": "^29.6.1",
    "ts-jest": "^29.1.0",
    "@types/jest": "^29.5.3",
    "@testing-library/react": "^14.0.0",
    "@testing-library/jest-dom": "^6.0.0",
    "supertest": "^6.3.3"
    ,
    "playwright": "^1.40.0"
  }
}
